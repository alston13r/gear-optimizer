{
  "homepage": "https://gmiclotte.github.io/gear-optimizer",
  "name": "gear-optimizer",
  "version": "0.7.5",
  "private": true,
  "dependencies": {
    "caniuse-lite": "^1.0.30001112",
    "gh-pages": "^2.2.0",
    "react": "^16.13.1",
    "react-cookie-banner": "^4.1.0",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "react-tooltip": "^3.11.6",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "git-info": "echo export default \"{\\\"logMessage\\\": \\\"$(git log -1 --oneline)\\\"}\"  > src/_git_commit.js",
    "start": "react-app-rewired start",
    "build": "npm run release-it; npm run git-info; react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "release-it": "release-it; echo export default \"{$(grep version -m1 package.json)}\"  > src/_version.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "release-it": {
    "npm": {
      "release": false
    }
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.6",
    "release-it": "^12.6.3"
  }
}
